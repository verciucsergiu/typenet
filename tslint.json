{
    "extends": [
        "tslint:latest",
        "tslint-blueprint"
    ],
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "array-type": [
            true,
            "generic"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "no-console": [
            false
        ],
        "eofline": false,
        "ordered-imports": false,
        "typedef": [
            true
        ],
        "typedef-whitespace": true,
        "object-literal-sort-keys": false,
        "object-literal-shorthand": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "no-empty": false,
        "no-unused-expression": false,
        "ban-types": [
            false,
            [
                "Object"
            ]
        ],
        "member-ordering": false
    }
}